            }
            var path = '@Url.Content("~/ProViews/CustomerEnquiryTracking")' + "?EnqId=" + EnqId 
            event.preventDefault();
            window.location.replace(path);
        });
        $('#btnFind').click(function () {
            var type = $("#type").val();
            if (type.toString() == "") {
                alert("Please Select Type");
                return false;
            }
            var path = '@Url.Content("~/ProViews/FindEnquiryDetail")' + "?Type=" + type + "&Dep=Mgt"  
            $("#CreateFindData").ejDialog({
                contentUrl: path,
                contentType: "ajax"
            });
            $("#CreateFindData").ejDialog("open");
        });
    }); 
</script>
