<style>
    #chevron {
        position: relative;
        text-align: center;
        padding: 12px;
        margin-bottom: 6px;
        height: 80px;
        width: 100px;
    }

        #chevron:before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            height: 100%;
            width: 51%;
            background: red;
            background: -webkit-linear-gradient(#30AA58,#7DD39A  ); /* For Safari 5.1 to 6.0 */
            background: -o-linear-gradient(#30AA58, #7DD39A  ); /* For Opera 11.1 to 12.0 */
            background: -moz-linear-gradient(#30AA58, #7DD39A  ); /* For Firefox 3.6 to 15 */
            background: linear-gradient(#30AA58, #7DD39A  ); /* Standard syntax */
            -webkit-transform: skew(0deg, 16deg);
            -moz-transform: skew(0deg, 16deg);
            -ms-transform: skew(0deg, 16deg);
            -o-transform: skew(0deg, 16deg);
            transform: skew(0deg, 16deg);
        }

        #chevron:after {
            content: '';
            position: absolute;
            top: 0;
            right: 0;
            height: 100%;
            width: 50%;
            background: red;
            background: -webkit-linear-gradient(#30AA58  ,#7DD39A  ); /* For Safari 5.1 to 6.0 */
            background: -o-linear-gradient(#30AA58,#7DD39A  ); /* For Opera 11.1 to 12.0 */
            background: -moz-linear-gradient(#30AA58,#7DD39A  ); /* For Firefox 3.6 to 15 */
            background: linear-gradient(#30AA58, #7DD39A); /* Standard syntax */
            -webkit-transform: skew(0deg, -16deg);
            -moz-transform: skew(0deg, -16deg);
            -ms-transform: skew(0deg, -16deg);
            -o-transform: skew(0deg, -16deg);
            transform: skew(0deg, -16deg);
        }
</style>
