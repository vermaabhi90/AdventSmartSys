        ctx.textAlign = 'center';

        ctx.font = "20px Arial";

        ctx.fillText(al + '%', cw * 0.5, ch * 0.5 + 2, cw);

        ctx.beginPath();

        ctx.arc(80, 80, 70, start, diff + start, false);

        ctx.stroke();

